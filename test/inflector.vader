Execute(Tests for Sanitize):
  AssertEqual ['some', 'text', 'to', 'work'], Sanitize('some text to work')
  AssertEqual ['some', 'text', 'to', 'work'], Sanitize('Some Text To Work')
  AssertEqual ['some', 'text', 'to', 'work'], Sanitize('SomeTextToWork')
  AssertEqual ['some', 'text', 'to', 'work'], Sanitize('someTextToWork')
  AssertEqual ['some', 'text', 'to', 'work'], Sanitize('Some_Text_To_Work')
  AssertEqual ['some', 'text', 'to', 'work'], Sanitize('SOME_TEXT_TO_WORK')
  AssertEqual ['some', 'text', 'to', 'work'], Sanitize('some_text_to_work')
  AssertEqual ['some', 'text', 'to', 'work'], Sanitize('_some_text_to_work')
  AssertEqual ['some', 'text', 'to', 'work'], Sanitize('some-text-to-work')

